<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.scbirs.trainingmanager.controllers.ControllerStage">
    <top>
        <ToolBar fx:id="toolbar" prefHeight="40.0" BorderPane.alignment="CENTER">
            <Button mnemonicParsing="false" text="TestButton"/>
        </ToolBar>
    </top>
    <center>
        <TabPane tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <Tab fx:id="tabTraining" text="Training">
            </Tab>
            <Tab fx:id="tabSeries" text="Series">
            </Tab>
        </TabPane>
    </center>
</BorderPane>
